{
  "name": "viking",
  "version": "2.0.0-rc2",
  "author": "Jonathan Bracy <jonbracy@gmail.com>",
  "description": "Viking.js is an open-source web framework for JavaScript. Inspired by Backbone.js & Ruby on Rails; it makes it easier to write client side JavaScript applications.",
  "url": "http://vikingjs.com",
  "repository": {
    "url": "git://github.com/malomalo/viking.git"
  },
  "keywords": [
    "backbone",
    "rails",
    "activerecord",
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.1.0",
    "sinon": "^9.0.2"
  },
  "scripts": {
    "test": "npx mocha -r @babel/register -r test/testSetup.js test/testHelper.js 'test/**/*.js'",
    "esdoc": "npx esdoc"
  },
  "dependencies": {
    "esdoc-ecmascript-proposal-plugin": "^1.0.0"
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./esdoc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      }
    ]
  }
}
