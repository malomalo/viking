{
    "sourceType": "module",
    "recurseDepth": 10,
    "source": {
        "include": ["./lib/"],
        "exclude": ["./node_modules/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/jsdoc-press"
    },
    "plugins": [
      "node_modules/jsdoc-export-default-interop/dist/index",
      "plugins/markdown"
    ],
    "templates": {
        "logo": "images/logo.png",
        "footer": false,
        "staticFiles": "docs-src/",
        "nav": {
            "order": ["class", "module"],
            "exclude": ["typedef"]
        },
        "stylesheets": [
          "./docs-src/stylesheets/jsdoc.css"
        ],
        "index": "./docs-src/index.md"
    }
}