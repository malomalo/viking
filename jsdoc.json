{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags": ["fires"]
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "opts": {
        "destination": "./out/",
        "recurse": true,
        "readme": "./README.md",
        "template": "node_modules/jsdoc-stock"
    },
    "templates": {
        "logo": "logo.png",
        "css": "styles/jsdoc.css",
        "default": {
          "staticFiles": {
            "include": ["static/", "logo.png"]
          }
        }
    }
}